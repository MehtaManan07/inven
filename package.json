{
  "name": "inven",
  "version": "1.0.0",
  "main": "./index.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "start:stag": "EXPO_USE_CUSTOM_NETWORK_PROXY=true APP_VARIANT=staging expo start --dev-client",
    "start:prod": "EXPO_USE_CUSTOM_NETWORK_PROXY=true APP_VARIANT=production expo start --dev-client",
    "android": "expo run:android",
    "android:stag": "yarn start:stag --android",
    "android:prod": "yarn start:prod --android",
    "ios": "expo run:ios",
    "ios:stag": "yarn start:stag --ios",
    "ios:prod": "yarn start:prod --ios",
    "web": "yarn web:stag",
    "web:stag": "yarn start:stag --web",
    "web:prod": "yarn start:prod --web",
    "build": "eas build -e",
    "build:latest": "eas build:run --latest",
    "submit:staging": "APP_VARIANT=staging eas submit -e staging",
    "submit:production": "APP_VARIANT=production eas submit -e production",
    "format": "yarn run format:check --write",
    "format:check": "yarn prettier 'src/**/*.(tsx|ts|js|jsx)'",
    "format:check:files": "yarn run format:check --list-different",
    "lint": "yarn lint:check --fix",
    "lint:check": "yarn eslint 'src/' --ext '.ts,.tsx,.js,.jsx'",
    "prepare": "husky install",
    "test": "jest",
    "update:staging": "APP_VARIANT=staging eas update --channel staging",
    "update:prod": "APP_VARIANT=production eas update --channel production",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "@tanstack/react-query": "^5.51.1",
    "expo": "~51.0.20",
    "expo-linear-gradient": "^13.0.2",
    "expo-splash-screen": "^0.27.5",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "^3.14.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "^15.4.0",
    "react-native-toast-message": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/metro-runtime": "^3.2.1",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "typescript": "^5.1.3"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn eslint --fix",
    "*.{js,jsx,ts,tsx,css}": "yarn prettier --write"
  }
}
